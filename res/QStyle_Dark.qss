QTreeView, QListView, QTableView{
    show-decoration-selected: 1;
	border: 1px solid #636363;
	background: #4D4D4D;
	selection-background-color: #4D4D4D;
	selection-color: #F0F0F0;
}

QTreeView::indicator{
	border: 1px solid #636363;
	background: #4D4D4D;
	selection-background-color: #4D4D4D;
	selection-color: #F0F0F0;
}

QTableView::item, QListView::item, QTreeView::item {
	padding: 5px; 
	margin: 0px; 
}
QTableView::item:selected, QListView::item:selected, QTreeView::item:selected {
	color: #F0F0F0;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4D4D4D, stop:1 #292929); 
}

QTableView::item:hover, QListView::item:hover, QTreeView::item:hover {
	color: #F0F0F0;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #636363, stop:1 #575757); 
}


QTreeWidget {
        border: 1px solid rgb(50, 50, 50);
        background-color: rgb(57, 58, 60);
}
QTreeWidget::item {
        height: 20px;
        color: white;
        border: none;
        background-color: transparent;
}
QTreeWidget::item:hover, QTreeWidget::branch:hover { /** 鼠标滑过 **/
        background-color: rgba(255, 255, 255, 40);
}
QTreeWidget::item:selected, QTreeWidget::branch:selected { /** 鼠标选中 **/
        background-color: rgb(0, 160, 230);
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4D4D4D, stop:1 #292929); 
}
QTreeWidget::branch {
        background-color: transparent;
}
QTreeWidget::branch:open:has-children {
        image: url(${AppDirPath}/skins/black/array_down.png);
}
QTreeWidget::branch:closed:has-children {
        image: url(${AppDirPath}/skins/black/add-line_horizontal.png);
}
QTreeWidget {
    show-decoration-selected: 1;
}





QTabWidget::tab-bar {
      left: 5px;/* move to the right by 5px */
      margin: 0px;
}

QTabBar::tab {
	border-top-left-radius:5px;
	border-top-right-radius:5px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4D4D4D, stop:1 #292929); 
	color: #F0F0F0;
	min-width: 60px;
      min-height: 18px;
	padding: 3px 8px 3px 8px;
	margin-left:3px;
}

QTabBar::tab:hover {
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #535353, stop:0.8 #3c3c3c, stop:1 #2c2c2c); 
}
QTabBar::tab:selected {
      border-top: 0px ;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #6c6c6c, stop:1 #474747); 
}

QTabBar::close-button {
    image: url(${AppDirPath}/skins/black/tab_close_button.png);
}
QTabBar::close-button:selected {
    image: url(${AppDirPath}/skins/black/tab_close_button_selected.png);
}
QTabBar::close-button:hover {
    image: url(${AppDirPath}/skins/black/tab_close_button_hover.png);
}
QTabBar::close-button:pressed {
    image: url(${AppDirPath}/skins/black/tab_close_button_pressed.png);
}

QStatusBar::item {
     border: 1px solid #636363;
     border-radius: 3px;
}
